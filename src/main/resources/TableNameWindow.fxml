      </Label>
      <Button fx:id="createTableButton" layoutX="193.0" layoutY="177.0" mnemonicParsing="false" onAction="#createTableTest" text="Utwórz tabelę" />
      <TextField fx:id="columnAmounts" layoutX="58.0" layoutY="132.0" prefHeight="36.0" prefWidth="198.0" />
      <Label layoutX="58.0" layoutY="90.0" prefHeight="36.0" prefWidth="221.0" text="Podaj ilość kolumn:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="closeTableWindow" layoutX="95.0" layoutY="177.0" mnemonicParsing="false" onAction="#closeAddTableView" text="Wstecz" />
   </children>
