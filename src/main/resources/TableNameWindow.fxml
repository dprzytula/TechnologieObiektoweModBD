      <TextField fx:id="tableNameField" layoutX="58.0" layoutY="43.0" prefHeight="36.0" prefWidth="198.0" />
      <Label layoutX="49.0" prefHeight="36.0" prefWidth="221.0" text="Podaj nazwę tabeli:">
         <font>
            <Font size="24.0" />
